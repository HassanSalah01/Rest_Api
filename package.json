  "scripts": {
    "build-jas": "npx tsc && jasmine",
    "start": "tsc && nodemon --watch './**/*.ts' --exec ts-node ./src/index.ts",
    "watch": "tsc-watch --esModuleInterop src/index.ts --outDir ./build --onSuccess \"node ./build/index.js\"",
    "clean": "rimraf build/",
    "build": " npx tsc",
    "start:prod": "npm run build && nodemon build/index.js",
    "format": "prettier --config .prettierrc.json src/**/*.ts --write",
    "test": "set ENV=test&& db-migrate --env test up  && npm run build && npx jasmine-ts && db-migrate --env test reset",
    "test2": "set ENV=test&&  db-migrate --env test reset",
    "lint": "eslint . --ext .ts",
    "jasmine": "jasmine-ts --config=./spec/support/jasmine.json"
  },
